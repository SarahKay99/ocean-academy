{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "oceanacademy-api-service"
  },
  "spec": {
    "type": "LoadBalancer",
    "selector": { "app": "oceanacademy-api" },
    "ports": [
      {
        "protocol": "TCP",
        "port": 80,
        "targetPort": 8080,
        "name": "http"
      }
    ]
  }
}
